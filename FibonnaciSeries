#include<bits/stdc++.h>
using namespace std;

int fib(int n,vector<int>& dp){
    if(n<=1) return n;
    if(dp[n]!=-1) return dp[n];
    return dp[n]=fib(n-1,dp)+fib(n-2,dp);
}

int main(){
    int n=5;
    vector<int> dp(n+1,-1);
    
    cout<<fib(n,dp)<<endl;
    return 0;
}


////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

#include<bits/stdc++.h>
using namespace std;

int main(){
  int n=5;
  vector<int> dp(n+1,-1);
  
  int prev  = 1;
  int prev2 = 0;

  for(int i=2; i<=n; i++){
    int curr_i = prev + prev2;
    prev2 = prev;
    prev = curr_i;
  }
  cout<<prev;
}
